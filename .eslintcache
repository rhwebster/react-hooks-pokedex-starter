tail\">\n      <div className={`pokemon-detail-image-background`}>\n        <div\n          className=\"pokemon-detail-image\"\n          style={{ backgroundImage: `url('${pokemon.imageUrl}')` }}\n        ></div>\n        <div>\n          <h1 className=\"bigger\">{pokemon.name}</h1>\n          {(!showEditPokeForm && pokemon.captured) && (\n            <button onClick={() => setShowEditPokeForm(true)}>Edit</button>\n          )}\n        </div>\n\n      </div>\n      {content}\n    </div>\n  );\n};\n\nexport default PokemonDetail;\n","/Users/ryan/Desktop/appAcademy/Wk15/react-hooks-pokedex-starter/src/components/CreatePokemonForm.js",[],"/Users/ryan/Desktop/appAcademy/Wk15/react-hooks-pokedex-starter/src/components/PokemonItems.js",[],"/Users/ryan/Desktop/appAcademy/Wk15/react-hooks-pokedex-starter/src/components/EditItemForm.js",[],"/Users/ryan/Desktop/appAcademy/Wk15/react-hooks-pokedex-starter/src/components/EditPokemonForm.js",[],{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":6,"column":7,"nodeType":"66","messageId":"67","endLine":6,"endColumn":11},{"ruleId":"64","severity":1,"message":"68","line":12,"column":7,"nodeType":"66","messageId":"67","endLine":12,"endColumn":13},{"ruleId":"64","severity":1,"message":"69","line":17,"column":7,"nodeType":"66","messageId":"67","endLine":17,"endColumn":10},{"ruleId":"64","severity":1,"message":"70","line":22,"column":7,"nodeType":"66","messageId":"67","endLine":22,"endColumn":13},{"ruleId":"71","severity":1,"message":"72","line":21,"column":6,"nodeType":"73","endLine":21,"endColumn":17,"suggestions":"74"},"no-native-reassign",["75"],"no-negated-in-lhs",["76"],"no-unused-vars","'load' is assigned a value but never used.","Identifier","unusedVar","'update' is assigned a value but never used.","'add' is assigned a value but never used.","'remove' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependenc